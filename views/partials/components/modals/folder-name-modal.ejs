<dialog class="<%= className %>">
  <form action="<%= formAction %>" method="post">
    <button
      class="close-modal-button"
      type="button"
      aria-label="Close write folder name modal"
    >
      x
    </button>
    <label>
      Folder Name
      <input type="text" name="folderName" />
      <button type="submit"><%= buttonName %></button>
    </label>
  </form>
</dialog>
